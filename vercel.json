{
    "rewrites": [
        {
            "source": "/",
            "destination": "@/pages/Login.jsx"
        },
        {
            "source": "/offers",
            "destination": "@/pages/offers.jsx"
        },
        {
            "source": "/home",
            "destination": "@/pages/home.jsx"
        },
        {
            "source": "/adm",
            "destination": "@/pages/admin.jsx"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/.*",
            "dest": "https://json-server-vercel-git-main-pascal-project.vercel.app/offers"
        }
    ]
}